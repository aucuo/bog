{
  "manifest_version": 3,
  "name": "Bogush solver",
  "author": "Jahor Å ykaviec",
  "version": "1.1",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["gpt.js", "answersProcessing.js", "correctAnswers.js", "env.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
